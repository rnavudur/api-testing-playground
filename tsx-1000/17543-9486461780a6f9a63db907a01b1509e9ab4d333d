{"code":"import{sql}from\"drizzle-orm\";import{pgTable,text,varchar,jsonb,timestamp,index}from\"drizzle-orm/pg-core\";import{createInsertSchema}from\"drizzle-zod\";import{z}from\"zod\";const apiRequests=pgTable(\"api_requests\",{id:varchar(\"id\").primaryKey().default(sql`gen_random_uuid()`),userId:varchar(\"user_id\").references(()=>users.id),method:text(\"method\").notNull(),url:text(\"url\").notNull(),headers:jsonb(\"headers\").$type().default({}),queryParams:jsonb(\"query_params\").$type().default({}),body:text(\"body\"),response:jsonb(\"response\").$type(),responseHeaders:jsonb(\"response_headers\").$type().default({}),status:text(\"status\"),statusCode:text(\"status_code\"),responseTime:text(\"response_time\"),createdAt:timestamp(\"created_at\").defaultNow()});const insertApiRequestSchema=createInsertSchema(apiRequests).omit({id:true,createdAt:true});const apiRequestConfigSchema=z.object({method:z.enum([\"GET\",\"POST\",\"PUT\",\"DELETE\",\"PATCH\"]),url:z.string().url(),headers:z.record(z.string()).default({}),queryParams:z.record(z.string()).default({}),body:z.string().optional()});const sessions=pgTable(\"sessions\",{sid:varchar(\"sid\").primaryKey(),sess:jsonb(\"sess\").notNull(),expire:timestamp(\"expire\").notNull()},table=>[index(\"IDX_session_expire\").on(table.expire)]);const users=pgTable(\"users\",{id:varchar(\"id\").primaryKey().default(sql`gen_random_uuid()`),email:varchar(\"email\").unique().notNull(),username:varchar(\"username\").unique().notNull(),firstName:varchar(\"first_name\"),lastName:varchar(\"last_name\"),password:varchar(\"password\").notNull(),profileImageUrl:varchar(\"profile_image_url\"),createdAt:timestamp(\"created_at\").defaultNow(),updatedAt:timestamp(\"updated_at\").defaultNow()});const insertUserSchema=createInsertSchema(users).omit({id:true,createdAt:true,updatedAt:true});const loginSchema=z.object({email:z.string().email(),password:z.string().min(6)});const signupSchema=z.object({email:z.string().email(),username:z.string().min(3).max(20),firstName:z.string().min(1).optional(),lastName:z.string().min(1).optional(),password:z.string().min(6),confirmPassword:z.string().min(6)}).refine(data=>data.password===data.confirmPassword,{message:\"Passwords don't match\",path:[\"confirmPassword\"]});export{apiRequestConfigSchema,apiRequests,insertApiRequestSchema,insertUserSchema,loginSchema,sessions,signupSchema,users};\n","warnings":[],"map":{"version":3,"mappings":"AAAA,OAAS,QAAW,cACpB,OAAS,QAAS,KAAM,QAAS,MAAO,UAAW,UAAa,sBAChE,OAAS,uBAA0B,cACnC,OAAS,MAAS,MAEX,MAAM,YAAc,QAAQ,eAAgB,CACjD,GAAI,QAAQ,IAAI,EAAE,WAAW,EAAE,QAAQ,sBAAsB,EAC7D,OAAQ,QAAQ,SAAS,EAAE,WAAW,IAAM,MAAM,EAAE,EACpD,OAAQ,KAAK,QAAQ,EAAE,QAAQ,EAC/B,IAAK,KAAK,KAAK,EAAE,QAAQ,EACzB,QAAS,MAAM,SAAS,EAAE,MAA8B,EAAE,QAAQ,CAAC,CAAC,EACpE,YAAa,MAAM,cAAc,EAAE,MAA8B,EAAE,QAAQ,CAAC,CAAC,EAC7E,KAAM,KAAK,MAAM,EACjB,SAAU,MAAM,UAAU,EAAE,MAAW,EACvC,gBAAiB,MAAM,kBAAkB,EAAE,MAA8B,EAAE,QAAQ,CAAC,CAAC,EACrF,OAAQ,KAAK,QAAQ,EACrB,WAAY,KAAK,aAAa,EAC9B,aAAc,KAAK,eAAe,EAClC,UAAW,UAAU,YAAY,EAAE,WAAW,CAChD,CAAC,EAEM,MAAM,uBAAyB,mBAAmB,WAAW,EAAE,KAAK,CACzE,GAAI,KACJ,UAAW,IACb,CAAC,EAMM,MAAM,uBAAyB,EAAE,OAAO,CAC7C,OAAQ,EAAE,KAAK,CAAC,MAAO,OAAQ,MAAO,SAAU,OAAO,CAAC,EACxD,IAAK,EAAE,OAAO,EAAE,IAAI,EACpB,QAAS,EAAE,OAAO,EAAE,OAAO,CAAC,EAAE,QAAQ,CAAC,CAAC,EACxC,YAAa,EAAE,OAAO,EAAE,OAAO,CAAC,EAAE,QAAQ,CAAC,CAAC,EAC5C,KAAM,EAAE,OAAO,EAAE,SAAS,CAC5B,CAAC,EAKM,MAAM,SAAW,QACtB,WACA,CACE,IAAK,QAAQ,KAAK,EAAE,WAAW,EAC/B,KAAM,MAAM,MAAM,EAAE,QAAQ,EAC5B,OAAQ,UAAU,QAAQ,EAAE,QAAQ,CACtC,EACC,OAAU,CAAC,MAAM,oBAAoB,EAAE,GAAG,MAAM,MAAM,CAAC,CAC1D,EAGO,MAAM,MAAQ,QAAQ,QAAS,CACpC,GAAI,QAAQ,IAAI,EAAE,WAAW,EAAE,QAAQ,sBAAsB,EAC7D,MAAO,QAAQ,OAAO,EAAE,OAAO,EAAE,QAAQ,EACzC,SAAU,QAAQ,UAAU,EAAE,OAAO,EAAE,QAAQ,EAC/C,UAAW,QAAQ,YAAY,EAC/B,SAAU,QAAQ,WAAW,EAC7B,SAAU,QAAQ,UAAU,EAAE,QAAQ,EACtC,gBAAiB,QAAQ,mBAAmB,EAC5C,UAAW,UAAU,YAAY,EAAE,WAAW,EAC9C,UAAW,UAAU,YAAY,EAAE,WAAW,CAChD,CAAC,EAEM,MAAM,iBAAmB,mBAAmB,KAAK,EAAE,KAAK,CAC7D,GAAI,KACJ,UAAW,KACX,UAAW,IACb,CAAC,EAEM,MAAM,YAAc,EAAE,OAAO,CAClC,MAAO,EAAE,OA